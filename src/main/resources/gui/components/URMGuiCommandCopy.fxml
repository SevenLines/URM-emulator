<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" spacing="5.0" fx:id="components">
         <children>
            <Label minWidth="-Infinity" text="T(" HBox.hgrow="ALWAYS" />
            <TextField id="edtReg1" fx:id="edtReg1" />
            <Label minWidth="-Infinity" text="," />
            <TextField id="edtReg2" fx:id="edtReg2" />
            <Label minWidth="-Infinity" text=")" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <styleClass>
            <String fx:value="command" />
            <String fx:value="copy" />
         </styleClass>
      </HBox>
      <Pane HBox.hgrow="ALWAYS" />
   </children>
</HBox>
