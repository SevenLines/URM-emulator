<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox prefHeight="36.0" prefWidth="167.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" fx:id="componentsWrapper">
         <children>
            <HBox alignment="CENTER"  fx:id="components" spacing="5.0">
               <children>
                  <Label minWidth="-Infinity" text="S(" />
                  <TextField id="edtReg" fx:id="edtReg" />
                  <Label minWidth="-Infinity" text=")" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <Button styleClass="remove" mnemonicParsing="false" text="x" textOverrun="CLIP" fx:id="btnRemove"/>
         </children>
         <styleClass>
            <String fx:value="command" />
            <String fx:value="add" />
         </styleClass>
      </HBox>
      <Pane HBox.hgrow="ALWAYS" />
   </children>
</HBox>
